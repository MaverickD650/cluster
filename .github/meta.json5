        "sourceUrl": "https://github.com/cloudnative-pg/cloudnative-pg/"
      },
      {
        "matchDepNames": ["freshrss"],
        "matchDatasources": ["docker"],
        "sourceUrl": "https://github.com/FreshRSS/FreshRSS"        
